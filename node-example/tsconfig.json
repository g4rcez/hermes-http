{
	"compilerOptions": {
		"outDir": "build",
		"module": "CommonJS",
		"target": "es5",
		"lib": [
			"esnext"
		],
		"sourceMap": true,
		"allowJs": false,
		"jsx": "react",
		"declaration": true,
		"moduleResolution": "node",
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"suppressImplicitAnyIndexErrors": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"include": [
		"src"
	]
}